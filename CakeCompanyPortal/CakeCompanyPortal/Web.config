	<loggingConfiguration name="Logging Application Block" tracingEnabled="true" defaultCategory="Operational" logWarningsWhenNoCategoriesMatch="true">

		<logFilters>
			<add enabled="true" type="Microsoft.Practices.EnterpriseLibrary.Logging.Filters.LogEnabledFilter, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" name="LogEnabled Filter" />
		</logFilters>

		<listeners>

			<add name="Operational Log File"
			  fileName="Logs\Cake-Company-Operational.log"
			  formatter="Operational Log File Format"
			  header="" footer=""
			  rollFileExistsBehavior="Increment" rollInterval="Day" timeStampPattern="yyyy-MM-dd" rollSizeKB="0"
			  traceOutputOptions="None"
			  filter="All"
			  type="Microsoft.Practices.EnterpriseLibrary.Logging.TraceListeners.RollingFlatFileTraceListener, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
			  listenerDataType="Microsoft.Practices.EnterpriseLibrary.Logging.Configuration.RollingFlatFileTraceListenerData, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />

			<add name="Developer Log File"
			  fileName="Logs\Cake-Company-Developer.log"
			  formatter="Developer Log File Format"
			  header="" footer=""
			  traceOutputOptions="None"
			  filter="All"
			  type="Microsoft.Practices.EnterpriseLibrary.Logging.TraceListeners.FlatFileTraceListener, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
			  listenerDataType="Microsoft.Practices.EnterpriseLibrary.Logging.Configuration.FlatFileTraceListenerData, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />

			<!--<add name="MPIArcGISCode Log File"
			  fileName="Logs\BT.PIPER.MPI.IntegrationService.ARCGISLogFile.log"
			  formatter="MPIArcGISCode Log File Format"
			  header="" footer=""
			  traceOutputOptions="None"
			  filter="All"
			  type="Microsoft.Practices.EnterpriseLibrary.Logging.TraceListeners.FlatFileTraceListener, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
			  listenerDataType="Microsoft.Practices.EnterpriseLibrary.Logging.Configuration.FlatFileTraceListenerData, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />-->

			<add name="System Application Log"
			  log="Application"
			  source="PIPeR-MPI Web Host"
			  formatter="System Application Log Format"
			  machineName=""
			  traceOutputOptions="None"
			  filter="All"
			  type="Microsoft.Practices.EnterpriseLibrary.Logging.TraceListeners.FormattedEventLogTraceListener, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
			  listenerDataType="Microsoft.Practices.EnterpriseLibrary.Logging.Configuration.FormattedEventLogTraceListenerData, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />

		</listeners>

		<formatters>

			<add name="Operational Log File Format"
			  template="{timestamp}{tab}{severity}{tab}{priority}{tab}{message}{tab}{title}"
			  type="Microsoft.Practices.EnterpriseLibrary.Logging.Formatters.TextFormatter, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />

			<add name="Developer Log File Format"
			  template="{timestamp}{tab}{priority}{tab}{severity}{tab}{message}{tab}{machine}{tab}{appDomain}{tab}{processId}{tab}{processName}{tab}{win32ThreadId}{tab}{threadName}"
			  type="Microsoft.Practices.EnterpriseLibrary.Logging.Formatters.TextFormatter, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />

			<!--<add name="MPIArcGISCode Log File Format"
			  template="{timestamp}{tab}{priority}{tab}{severity}{tab}{message}{tab}{machine}{tab}{appDomain}{tab}{processId}{tab}{processName}{tab}{win32ThreadId}{tab}{threadName}"
			  type="Microsoft.Practices.EnterpriseLibrary.Logging.Formatters.TextFormatter, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />-->

			<add name="System Application Log Format"
			  template="Timestamp: {timestamp}&#xD;&#xA;Message: {message}&#xD;&#xA;Category: {category}&#xD;&#xA;Priority: {priority}&#xD;&#xA;EventId: {eventid}&#xD;&#xA;Severity: {severity}&#xD;&#xA;Title:{title}&#xD;&#xA;Machine: {machine}&#xD;&#xA;Application Domain: {appDomain}&#xD;&#xA;Process Id: {processId}&#xD;&#xA;Process Name: {processName}&#xD;&#xA;Win32 Thread Id: {win32ThreadId}&#xD;&#xA;Thread Name: {threadName}&#xD;&#xA;Extended Properties: {dictionary({key} - {value}&#xD;&#xA;)}"
			  type="Microsoft.Practices.EnterpriseLibrary.Logging.Formatters.TextFormatter, Microsoft.Practices.EnterpriseLibrary.Logging, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />

		</formatters>

		<categorySources>

			<add switchValue="All" name="Operational">
				<listeners>
					<add name="Operational Log File" />
				</listeners>
			</add>

			<add switchValue="All" name="Developer">
				<listeners>
					<add name="Developer Log File" />
				</listeners>
			</add>

			<!--<add switchValue="All" name="MPIArcGISCode">
				<listeners>
					<add name="MPIArcGISCode Log File" />
				</listeners>
			</add>-->

			<add switchValue="All" name="System">
				<listeners>
					<add name="System Application Log" />
				</listeners>
			</add>

		</categorySources>

		<specialSources>
			<allEvents switchValue="All" name="All Events" />
			<notProcessed switchValue="All" name="Unprocessed Category" />
			<errors switchValue="All" name="Logging Errors &amp; Warnings">
				<listeners>
					<add name="System Application Log" />
				</listeners>
			</errors>
		</specialSources>

	</loggingConfiguration>
</configuration>